{
  "name": "curl-to-json-parser",
  "version": "0.1.0",
  "description": "Convert a multiline curl command string into a structured, parsable JSON object.",
  "keywords": [
    "curl",
    "parser",
    "json",
    "http",
    "request",
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jayant-Jeet/curl-to-json-parser.git"
  },
  "author": "Jayant Jeet <jjtomar@halfbloodcoder.com>",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "bin": {
    "curl-to-json-parser": "dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "test": "node tests/run-tests.mjs",
    "test:watch": "vitest",
    "test:smoke": "node scripts/smoke.mjs",
    "lint": "echo Skipping lint for now",
    "prepublishOnly": "npm run build && npm test"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  }
}
